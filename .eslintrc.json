{
  "extends": [
    "eslint:recommended",
    "prettier",
    "prettier/react",
    "jest:recommended"
  ],
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "react/react-in-jsx-scope": "off"
  },
  "globals": {
    "React": "writable"
  },
  "plugins": [
    "jest"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}